################################################################

set logscale x
unset xlabel
unset ylabel
unset title

set term pdf size 6.5,5
outputtitle= sprintf("%s%d%s","Approx_N3LO_g_vs_eta_xi=", xi,".pdf")
set output outputtitle
plottitle= sprintf("%s%d","Approx, O({/Symbol a}_s^3), {/Symbol x}=", xi)
set title plottitle
set xlabel "{/Symbol h}" font ",15"
set ylabel "xC_{L,g}^{(3,0)}" font ",15"
set key left top

set format x "10^{%T}"

plot filename2 u 1:2 w l  title "asymptotic" lc "green" lw 1 dt 2 noautoscale, filename2 u 1:3 w l title "threshold" lc "purple" lw 1 dt 4, filename2 u 1:4 w l title "approximation" dt 1 lw 1 lc "red"

