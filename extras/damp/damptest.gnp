set size 1,0.30
set origin 0,0
set tmargin 0
set lmargin at screen 0.1
set bmargin at screen 0.09
set rmargin at screen 0.95

set xlabel "{/Symbol h}" font ",15"
set ylabel "dampings" font ",15"
set yrange [-0.1:1.1]
set ytics 0, 0.25,1
unset key

plot  filename u 1:11 w l dt 2 lw 1 lc "green"
