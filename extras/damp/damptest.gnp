set logscale x
unset xlabel
unset ylabel
unset title

set term pdf size 6.5,5
outputtitle= sprintf("%s%d%s","damptest_g_N2LO_vs_eta_xi=", xi,".pdf")
set output outputtitle
multiplottitle= sprintf("%s%d","Approximation F_L for gluon, O({/Symbol a}_s^2), {/Symbol x}=", xi)
set multiplot title multiplottitle font ",15"
#set xlabel "{/Symbol h}" font ",15"
set ylabel "xC_{L,g}^{(2,0)}" font ",15"
set key left top
set size 1,0.5
set origin 0,0.2
set lmargin at screen 0.1
set tmargin at screen 0.95
set rmargin at screen 0.95
set bmargin at screen 0.35
set format x "10^{%T}"

plot filename u 1:2 w l title "exact" lc "orange" lw 1 dt 1,filename u 1:3 w l title "asymptotic" lc "green" lw 1 dt 2, filename u 1:4 w l title "threshold" lc "purple" lw 1 dt 4, filename u 1:5 w l title "approximation" dt 1 lw 1.2 lc "red"
